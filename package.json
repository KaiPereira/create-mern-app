{
  "name": "create-mern-ts-next-app",
  "version": "1.1.0",
  "description": "Create Mern TS Next App is a great boilerplate for your next app. It's a quick and simple way to setup your project",
  "bin": {
    "create-mern-ts-next-app": "./bin/create-app.js"
  },
  "keywords": [
    "react",
    "nextjs",
    "mern",
    "typescript",
    "express",
    "node",
    "mongodb",
    "mongoose"
  ],
  "author": "Kai Pereira",
  "license": "MIT",
  "homepage": "https://github.com/KaiPereira/create-mern-app-kaipereira",
  "repository": {
    "type": "git",
    "url": "https://github.com/KaiPereira/create-mern-app-kaipereira.git"
  },
  "bugs": "https://github.com/KaiPereira/create-mern-app-kaipereira/issues",
  "scripts": {
    "start": "npm run clean && parcel serve public/index.html --dist-dir development -p 3000",
    "build": "rimraf ./build && parcel build public/index.html --dist-dir ./build --public-url ./",
    "test": "jest",
    "clean": "rimraf ./development && rimraf ./.parcel-cache",
    "prettify": "npx prettier --write ./src/"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@parcel/reporter-bundle-analyzer": "^2.0.0-beta.2",
    "@parcel/transformer-sass": "^2.0.0-beta.2",
    "@testing-library/dom": "^7.30.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "autoprefixer": "^10.2.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "jest-cli": "^26.6.3",
    "parcel": "^2.0.0-beta.2",
    "prettier": "2.2.1"
  },
  "dependencies": {
    "jest-styled-components": "^7.0.3",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "> 1%",
          "last 4 versions",
          "ie >= 9"
        ]
      }
    }
  }
}
